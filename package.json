    "console": "npx hardhat console",
    "test": "npx hardhat test",
    "test:coverage": "npx hardhat coverage",
    "hardhat": "npx hardhat",
    "solhint": "npx solhint -f table contracts/**/*.sol && npx solhint -f table contracts/*.sol",
    "contract:size": "npx hardhat size-contracts",
    "contract:deploy": "TASK=deploy npx hardhat run scripts/main.ts",
    "contract:upgrade": "TASK=upgrade npx hardhat run scripts/main.ts",
    "contract:query": "TASK=query npx hardhat run scripts/main.ts",
    "contract:playground": "npx hardhat run scripts/playground.ts",
    "contract:useGnosis": "TASK=useGnosis npx hardhat run scripts/main.ts",
    "contract:stakePool:updateConfig": "TASK=stakePool:updateConfig npx hardhat run scripts/main.ts",
    "lint:check:ts": "npx prettier --check *.ts {scripts,test,agents}/**/*.{ts,js}",
    "lint:check:solidity": "npx prettier --check {contracts,certora}/**/*.sol && npx prettier --check contracts/*.sol",
    "lint:check": "yarn lint:check:ts && yarn lint:check:solidity && yarn solhint",
    "lint:fix:ts": "npx prettier --write *.ts {scripts,test,agents}/**/*.{ts,js}",
    "lint:fix:solidity": "npx prettier --write {contracts,certora}/**/*.sol && npx prettier --write contracts/*.sol",
    "lint": "yarn lint:fix:ts && yarn lint:fix:solidity"
  },
